    "brace-expansion": "^2.0.1",
    "foreground-child": "^3.1.1",
    "emoji-regex": "^8.0.0",
    "has": "^1.0.3",
    "function-bind": "^1.1.1",
    "estree-walker": "^2.0.2",
    "is-core-module": "^2.12.1",
    "minipass": "^7.0.2",
    "path-parse": "^1.0.7",
    "jackspeak": "^2.2.1",
    "path-scurry": "^1.10.1",
    "randombytes": "^2.1.0",
    "picomatch": "^2.3.1",
    "serialize-javascript": "^6.0.1",
    "safe-buffer": "^5.2.1",
    "resolve": "^1.22.2",
    "source-map-support": "^0.5.21",
    "smob": "^1.4.0",
    "source-map": "^0.6.1",
    "commander": "^2.20.3",
    "terser": "^5.19.1",
    "isexe": "^2.0.0",
    "supports-preserve-symlinks-flag": "^1.0.0",
    "strip-ansi-cjs": "^6.0.1",
    "wrap-ansi-cjs": "^7.0.0",
    "string-width-cjs": "^4.2.3"

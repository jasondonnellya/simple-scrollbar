    "rollup": "^3.26.3",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "type": "module",
  "bugs": {
    "url": "https://github.com/jasondonnellya/stylable-scrollbar/issues"
  },
  "homepage": "https://github.com/jasondonnellya/stylable-scrollbar#readme",
  "directories": {
    "example": "example"
  },
  "dependencies": {
